{
    "manifestVersion": 1,
    "id": "Angular-AzureDevopc-POC2",
    "publisher": "DrMueller2",
    "version": "1.0.158",
    "name": "Angular Azure DevOps POC",
    "description": "A proof of concept for Angular with Azure DevOps extensions",
    "public": false,
    "icons": {
        "default": "src/assets/M_256.png"
    },
    "categories": [
        "Azure Repos"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "HelloWorldHub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Hello World Hub",
                "uri": "vss/hubs/test-hub.html",
                "iconAsset": "DrMueller2.Angular-AzureDevopc-POC/assets/M_98.png",
                "_sharedData": {
                    "assets": [
                        "DrMueller2.Angular-AzureDevopc-POC/assets/M_98.png"
                    ]
                }
            }
        },
        {
            "id": "HelloWorldWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Hello World Widget",
                "description": "My first widget",
                "catalogIconUrl": "assets/M_98.png",
                "previewImageUrl": "assets/M_98.png",
                "uri": "vss/dashboards/test-dashboard.html",
                "supportedSizes": [
                    {
                        "rowSpan": 1,
                        "columnSpan": 2
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        }
    ],
    "files": [
        {
            "path": "dist",
            "addressable": true,
            "packagePath": "angular-app"
        },
        {
            "path": "src/vss",
            "addressable": true,
            "packagePath": "vss"
        },
        {
            "path": "src/assets",
            "addressable": true,
            "packagePath": "assets"
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}